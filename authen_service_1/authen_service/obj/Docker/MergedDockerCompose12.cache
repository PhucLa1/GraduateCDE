name: dockercompose14052945388082341392
services:
  authen_service:
    build:
      context: C:\Users\Hong Phuc\Desktop\Study\GraduateCDE\authen_service_1\authen_service
      dockerfile: Dockerfile
    container_name: authen-service
    depends_on:
      authenservicedb:
