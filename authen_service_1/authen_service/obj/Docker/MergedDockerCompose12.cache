      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
      DB_HOST: authenservicedb
      DB_NAME: authendb
      DB_SA_PASSWORD: password@12345#
    image: authenservice
    networks:
      authenservice: null
    ports:
      - mode: ingress
        target: 80
        published: "8002"
